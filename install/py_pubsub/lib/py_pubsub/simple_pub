#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'py-pubsub','console_scripts','simple_pub'
__requires__ = 'py-pubsub'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('py-pubsub', 'console_scripts', 'simple_pub')()
    )
